{
  "instant": {
    "epochSecond": {
      "$resolver": "timestamp",
      "epoch": {
        "unit": "secs",
        "rounded": true
      }
    },
    "nanoOfSecond": {
      "$resolver": "timestamp",
      "epoch": {
        "unit": "secs.nanos"
      }
    }
  },
  "level": {
    "$resolver": "level",
    "field": "name"
  },
  "loggerName": {
    "$resolver": "logger",
    "field": "name"
  },
  "message": {
    "$resolver": "message",
    "stringified": true
  },
  "owner": {
    "$resolver": "owner"
  }
}
